{
      "version": "0.2.0",
      "configurations": [
        {
          "name": "Debug ChainedDecos",
          "type": "cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/cmake-build-debug/ChainedDecos.exe", 
          "args": [],
          "stopAtEntry": true,
          "cwd": "${workspaceFolder}",
          "environment": [],
          "externalConsole": false,
          "MIMode": "gdb",
          "miDebuggerPath": "C:/msys64/mingw64/bin/gdb.exe",
          "setupCommands": [
            {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
            },
            {
              "description": "Set Disassembly Flavor to Intel",
              "text": "-gdb-set disassembly-flavor intel",
              "ignoreFailures": true
            }
          ],
          "preLaunchTask": "",
          "sourceFileMap": {}
        }
      ]
}