{
      "version": "0.2.0",
      "configurations": [
        {
          "name": "Debug with LLDB",
          "type":"cppdbg",
          "request": "launch",
          "program": "${workspaceFolder}/build/ChainedDecos.exe",
          "args": [],
          "cwd": "${workspaceFolder}",
          "stopAtEntry": true,
          "lldbPath": "E:/LLVM/bin/lldb.exe"
