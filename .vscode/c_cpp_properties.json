{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${default}",
                "${workspaceFolder}/src",
                "${workspaceFolder}/include",
                "${workspaceFolder}/cmake-build-debug/_deps/*/include",
                "${workspaceFolder}/cmake-build-debug/_deps/raylib-src/src",
                "${workspaceFolder}/cmake-build-debug/_deps/nlohmann_json-src/include"
            ],
            "defines": [
                "_DEBUG",
                "DEBUG",
                "UNICODE",
                "_UNICODE",
                "PROJECT_ROOT_DIR=\"${workspaceFolder}\""
            ],
            "compilerPath": "E:/LLVM/bin/clang++.exe",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "windows-clang-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}